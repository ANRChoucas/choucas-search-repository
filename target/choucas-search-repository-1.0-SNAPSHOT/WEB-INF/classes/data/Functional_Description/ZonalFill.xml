<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessOfferings xmlns:wps="http://www.opengis.net/wps/2.0.0" xmlns:ows="http://www.opengis.net/ows/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/2.0.0 ../wpsDescribeProcess.xsd">
   <wps:ProcessOffering outputTransmission="value reference">
      <wps:Process>
         <ows:Identifier>ZonalFill</ows:Identifier>
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/process-profile/concept_functionality" xlink:href="http://www.semanticweb.org/home/ontologies/2018/6/untitled-ontology-6#Zone_treatment_service" />
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/ def/process/description/documentation" xlink:href="https://52north.org/" />
         <ows:Title>ZonalFill_sa</ows:Title>
         <ows:Abstract>Fills zones using the minimum cell value from a weight raster along the zone boundary. Uses ArcObjects library - Spatial Analyst</ows:Abstract>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>in_zone_raster</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#Zone" />
            <ows:Title>in zone raster</ows:Title>
            <ows:Abstract>The input raster that defines the zones that are to be filled.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Input>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>in_weight_raster</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#WeightZone" />
            <ows:Title>in weight raster</ows:Title>
            <ows:Abstract>Weight to be assigned to each zone.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Input>
         <wps:Output>
            <ows:Identifier>out_raster</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Output" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#ZonalFill" />
            <ows:Title>out raster</ows:Title>
            <ows:Abstract>The raster to be created.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Output>
         <ows:Metadata xlink:role="http://choucas.ign.fr/spec/wps /2.0/def/process-profile/Non-functional_properties" xlink:href="./NonFunctional_Description/NFZonalFill.xml" />
      </wps:Process>
   </wps:ProcessOffering>
</wps:ProcessOfferings>
