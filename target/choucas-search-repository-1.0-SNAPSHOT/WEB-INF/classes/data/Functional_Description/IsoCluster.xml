<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessOfferings xmlns:wps="http://www.opengis.net/wps/2.0.0" xmlns:ows="http://www.opengis.net/ows/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/2.0.0 ../wpsDescribeProcess.xsd">
   <wps:ProcessOffering outputTransmission="value reference">
      <wps:Process>
         <ows:Identifier>IsoCluster</ows:Identifier>
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/process-profile/concept_functionality" xlink:href="http://www.semanticweb.org/home/ontologies/2018/6/untitled-ontology-6#Calculation_statistics_service" />
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/ def/process/description/documentation" xlink:href="https://52north.org/" />
         <ows:Title>IsoCluster_sa</ows:Title>
         <ows:Abstract>Uses an isodata clustering algorithm to determine the characteristics of the natural groupings of cells in multidimensional attribute space and stores the results in an output ASCII signature file. Uses ArcObjects library - Spatial Analyst</ows:Abstract>
         <wps:Input minOccurs="1" maxOccurs="1000">
            <ows:Identifier>in_raster_bands</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#RasterBands" />
            <ows:Title>input raster bands</ows:Title>
            <ows:Abstract>Input raster bands.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Input>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>number_classes</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#NumberClasses" />
            <ows:Title>number classes</ows:Title>
            <ows:Abstract>Number of classes into which to group the cells.</ows:Abstract>
            <wps:LiteralData>
               <wps:Format mimeType="text/plain" />
               <LiteralDataDomain>
                  <ows:AnyValue />
                  <ows:DataType ows:reference="http://www.w3.org/2001/XMLSchema#integer">Integer</ows:DataType>
               </LiteralDataDomain>
            </wps:LiteralData>
         </wps:Input>
         <wps:Input minOccurs="0" maxOccurs="1">
            <ows:Identifier>number_iterations</ows:Identifier>
            <ows:Title>number iterations</ows:Title>
            <ows:Abstract>Number of iterations of the clustering process to run. The default is 20.</ows:Abstract>
            <wps:LiteralData>
               <wps:Format mimeType="text/plain" />
               <LiteralDataDomain>
                  <ows:AnyValue />
                  <ows:DataType ows:reference="http://www.w3.org/2001/XMLSchema#integer">Integer</ows:DataType>
               </LiteralDataDomain>
            </wps:LiteralData>
         </wps:Input>
         <wps:Input minOccurs="0" maxOccurs="1">
            <ows:Identifier>min_class_size</ows:Identifier>
            <ows:Title>min class size</ows:Title>
            <ows:Abstract>Minimum number of cells in a valid class. The default is 20.</ows:Abstract>
            <wps:LiteralData>
               <wps:Format mimeType="text/plain" />
               <LiteralDataDomain>
                  <ows:AnyValue />
                  <ows:DataType ows:reference="http://www.w3.org/2001/XMLSchema#integer">Integer</ows:DataType>
               </LiteralDataDomain>
            </wps:LiteralData>
         </wps:Input>
         <wps:Input minOccurs="0" maxOccurs="1">
            <ows:Identifier>sample_interval</ows:Identifier>
            <ows:Title>sample interval</ows:Title>
            <ows:Abstract>The interval to use for sampling. Default is 10.</ows:Abstract>
            <wps:LiteralData>
               <wps:Format mimeType="text/plain" />
               <LiteralDataDomain>
                  <ows:AnyValue />
                  <ows:DataType ows:reference="http://www.w3.org/2001/XMLSchema#integer">Integer</ows:DataType>
               </LiteralDataDomain>
            </wps:LiteralData>
         </wps:Input>
         <wps:Output>
            <ows:Identifier>out_signature_file</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Output" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#SignatureIsoCluster" />
            <ows:Title>out signature file</ows:Title>
            <ows:Abstract>Output signature file. A ".gsg" extension is required.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/txt" default="true" />
            </wps:ComplexData>
         </wps:Output>
         <ows:Metadata xlink:role="http://choucas.ign.fr/spec/wps /2.0/def/process-profile/Non-functional_properties" xlink:href="./NonFunctional_Description/NFIsoCluster.xml" />
      </wps:Process>
   </wps:ProcessOffering>
</wps:ProcessOfferings>
