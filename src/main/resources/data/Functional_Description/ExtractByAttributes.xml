<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessOfferings xmlns:wps="http://www.opengis.net/wps/2.0.0" xmlns:ows="http://www.opengis.net/ows/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/2.0.0 ../wpsDescribeProcess.xsd">
   <wps:ProcessOffering outputTransmission="value reference">
      <wps:Process>
         <ows:Identifier>ExtractByAttributes</ows:Identifier>
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/process-profile/concept_functionality" xlink:href="http://www.semanticweb.org/home/ontologies/2018/6/untitled-ontology-6#Subassembly_definition_service" />
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/ def/process/description/documentation" xlink:href="https://52north.org/" />
         <ows:Title>ExtractByAttributes_sa</ows:Title>
         <ows:Abstract>Extracts the cells of a raster based on a logical query. Uses ArcObjects library - Spatial Analyst</ows:Abstract>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>in_raster</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#Raster" />
            <ows:Title>in raster</ows:Title>
            <ows:Abstract>The input raster from which cells will be extracted.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Input>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>where_clause</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#WhereClause" />
            <ows:Title>where clause</ows:Title>
            <ows:Abstract>An SQL expression used to select a subset of raster cells. For information on SQL syntax, see SQL Reference.</ows:Abstract>
            <wps:LiteralData>
               <wps:Format mimeType="text/plain" />
               <LiteralDataDomain>
                  <ows:AnyValue />
                  <ows:DataType ows:reference="http://www.w3.org/2001/XMLSchema#string">String</ows:DataType>
               </LiteralDataDomain>
            </wps:LiteralData>
         </wps:Input>
         <wps:Output>
            <ows:Identifier>out raster</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Output" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#ExtractionByAttributes" />
            <ows:Title>out_raster</ows:Title>
            <ows:Abstract>The raster to be created.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Output>
         <ows:Metadata xlink:role="http://choucas.ign.fr/spec/wps /2.0/def/process-profile/Non-functional_properties" xlink:href="./NonFunctional_Description/NFExtractByAttributes.xml" />
      </wps:Process>
   </wps:ProcessOffering>
</wps:ProcessOfferings>
