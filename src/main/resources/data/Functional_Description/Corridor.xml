<?xml version="1.0" encoding="UTF-8"?>
<wps:ProcessOfferings xmlns:wps="http://www.opengis.net/wps/2.0.0" xmlns:ows="http://www.opengis.net/ows/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wps/2.0.0 ../wpsDescribeProcess.xsd">
   <wps:ProcessOffering outputTransmission="value reference">
      <wps:Process>
         <ows:Identifier>Corridor</ows:Identifier>
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/process-profile/concept_functionality" xlink:href="http://www.semanticweb.org/home/ontologies/2018/6/untitled-ontology-6#Distance_sum_calculation_service" />
         <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/ def/process/description/documentation" xlink:href="https://52north.org/" />
         <ows:Title>Corridor_sa</ows:Title>
         <ows:Abstract>Calculates the sum of accumulative costs for two input accumulative cost raster. Uses ArcObjects library - Spatial Analyst</ows:Abstract>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>in_distance_raster1</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#CostDistance" />
            <ows:Title>in distance raster1</ows:Title>
            <ows:Abstract>An input raster that was the output accumulated cost raster from a global cost function.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Input>
         <wps:Input minOccurs="1" maxOccurs="1">
            <ows:Identifier>in_distance_raster2</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Input" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#CostDistance" />
            <ows:Title>in distance raster2</ows:Title>
            <ows:Abstract>An input raster that was the output accumulated cost raster from a global cost function.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Input>
         <wps:Output>
            <ows:Identifier>out_raster</ows:Identifier>
            <ows:Metadata xlink:role="http://www.opengis.net/spec/wps/2.0/def/concept_Output" xlink:href="http://www.semanticweb.org/halilali/ontologies/2020/0/untitled-ontology-25#Corridor" />
            <ows:Title>out raster</ows:Title>
            <ows:Abstract>The raster to be created.</ows:Abstract>
            <wps:ComplexData>
               <wps:Format MimeType="application/img" default="true" />
               <wps:Format MimeType="application/GeoTIFF" />
            </wps:ComplexData>
         </wps:Output>
         <ows:Metadata xlink:role="http://choucas.ign.fr/spec/wps /2.0/def/process-profile/Non-functional_properties" xlink:href="./NonFunctional_Description/NFCorridor.xml" />
      </wps:Process>
   </wps:ProcessOffering>
</wps:ProcessOfferings>
